<div class="row">
  <div class="col-xs-8 col-xs-offset-2">
    <h1 align="center">Users</h1>
    <% @users.each do |user| %>
      <ul class="user">
        <div class="row">
          <div class="well col-md-4 col-md-offset-4">
            <li class="user-title">
              <%= link_to user.username, user_path(user) %>
            </li>
          </div>
        </div>
      </ul>
    <% end %>
  </div>
</div>