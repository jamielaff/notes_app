<div class="col-xs-8 col-xs-offset-2">
  <h1>Edit Note</h1>
</div>

<div id="form_container">
  <%= render partial: 'form' %>
</div>

<div class="row">
  <div class="col-xs-8 col-xs-offset-2">
    <%= link_to 'Back', root_path, class: "btn btn-primary btn-large" %>
    <%= link_to "Delete", note_path(@note), method: :delete, data: { confirm: "Are you sure you want to delete this note?"}, class: "btn btn-danger btn-large" %>
  </div>
</div>
